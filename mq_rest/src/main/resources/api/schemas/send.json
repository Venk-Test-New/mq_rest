{
  "$schema": "http://json-schema.org/draft-03/schema#",
  "type": "object",
  "properties": {
    "headers": {
      "type": "object",
      "required": false,
      "properties": {
        "messageId": {
          "type": "string",
          "required": true
        },
        "lockId": {
          "type": "string",
          "required": true
        },
        "deliveryDelay": {
          "type": "integer",
          "range": "0-900000",
          "required": false
        }
      }
    },
    "body": {
      "type": "string",
      "required": true
    }
  }
}