{
  "$schema": "http://json-schema.org/draft-03/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "messageId": {
        "type": "string",
        "required": true
      },
      "lockId": {
        "type": "string",
        "required": true
      },
      "status": {
        "type": "string",
        "required": true,
        "enum": [
          "successful",
          "failure"
        ]
      },
      "statusMessage": {
        "type": "string",
        "required": true
      }
    }
  }
}
